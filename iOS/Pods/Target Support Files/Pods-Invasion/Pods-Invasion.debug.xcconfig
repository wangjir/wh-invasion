GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/BlocksKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/GLFoundation.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Masonry.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SwiftDate.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SwiftValidator.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SwiftyJSON.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SwiftyUserDefaults.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/libextobjc.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "BlocksKit" -framework "GLFoundation" -framework "Masonry" -framework "SwiftDate" -framework "SwiftValidator" -framework "SwiftyJSON" -framework "SwiftyUserDefaults" -framework "libextobjc"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Invasion
PODS_ROOT = ${SRCROOT}/Pods